# Google Veo 3 Production Environment Variables
# Copy this file to .env.production and fill in your actual values

# Google AI API Configuration
VITE_GOOGLE_AI_API_KEY=your_google_ai_api_key_here
VITE_VEO3_API_BASE_URL=https://api.google.ai/veo3

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Storage Configuration (for future cloud storage integration)
VITE_VEO3_VIDEO_BUCKET=veo3-videos
VITE_VEO3_THUMBNAIL_BUCKET=veo3-thumbnails

# Feature Flags
VITE_ENABLE_VEO3_PRODUCTION=true
VITE_ENABLE_VEO3_ANALYTICS=true
VITE_ENABLE_VEO3_QUOTAS=true

# Performance Configuration
VITE_VEO3_MAX_CONCURRENT_JOBS=5
VITE_VEO3_STATUS_UPDATE_INTERVAL=3000

# Monitoring and Analytics
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_USER_ANALYTICS=true

# Security Configuration
VITE_ENABLE_RATE_LIMITING=true
VITE_ENABLE_QUOTA_ENFORCEMENT=true
VITE_ENABLE_API_LOGGING=true

# Development/Testing (set to false in production)
VITE_ENABLE_DEBUG_LOGGING=false
VITE_SIMULATE_VEO3_API=false
